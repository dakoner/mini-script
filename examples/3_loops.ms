// Example 3: Loops
print("=== Example 3: Loops ===");

print("While loop countdown:");
i = 5;
while (i > 0) {
    print("Countdown:", i);
    i = i - 1;
}
print("Blast off!");

print("For loop (squares):");
for (j = 1; j <= 5; j = j + 1) {
    square = j * j;
    print(j, "squared is", square);
}

print("Example 3 completed!");