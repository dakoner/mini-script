// Test 4: Comparison Operators
print("=== Test 4: Comparison Operators ===");

a = 10;
b = 5;
c = 10;

// Equal and not equal
print("a =", a, ", b =", b, ", c =", c);
print("a == b:", a == b);
print("a == c:", a == c);
print("a != b:", a != b);
print("a != c:", a != c);

// Less than and greater than
print("a > b:", a > b);
print("a < b:", a < b);
print("b < a:", b < a);

// Less than or equal, greater than or equal
print("a >= c:", a >= c);
print("b <= a:", b <= a);
print("a <= b:", a <= b);

// String comparisons
str1 = "apple";
str2 = "banana";
str3 = "apple";

print("String comparisons:");
print("str1 =", str1, ", str2 =", str2, ", str3 =", str3);
print("str1 == str2:", str1 == str2);
print("str1 == str3:", str1 == str3);
print("str1 != str2:", str1 != str2);

// Boolean comparisons
bool1 = true;
bool2 = false;
bool3 = true;

print("Boolean comparisons:");
print("bool1 == bool2:", bool1 == bool2);
print("bool1 == bool3:", bool1 == bool3);
print("bool1 != bool2:", bool1 != bool2);

