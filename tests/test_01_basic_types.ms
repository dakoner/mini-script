// Test 1: Basic Variables and Data Types
print("=== Test 1: Basic Variables and Data Types ===");

// Numbers (integers and floating point)
int_var = 42;
float_var = 3.14159;
negative_num = -17;

print("Integer:", int_var);
print("Float:", float_var);
print("Negative:", negative_num);

// Strings
simple_string = "Hello, World!";
empty_string = "";
string_with_spaces = "   spaces   ";

print("Simple string:", simple_string);
print("Empty string:", empty_string);
print("String with spaces:", string_with_spaces);

// Booleans
bool_true = true;
bool_false = false;

print("Boolean true:", bool_true);
print("Boolean false:", bool_false);

// Variable reassignment
test_var = 10;
print("Before reassignment:", test_var);
test_var = 20;
print("After reassignment:", test_var);

print("Test 1: PASSED");
print("");
