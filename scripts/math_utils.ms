// math_utils.ms - A simple math utilities module

function add(int a, int b) {
    return a + b;
}

function multiply(int a, int b) {
    return a * b;
}

function square(int x) {
    return x * x;
}

// Set some module variables
module_version = 1;
module_name = "MathUtils";

print("Math utilities module loaded!");
