// Example 6: Boolean logic
print("=== Example 6: Boolean logic ===");

a = true;
b = false;
print("a =", a, ", b =", b);
print("a && b =", a && b);
print("a || b =", a || b);
print("!a =", !a);

x = 5;
y = 10;
print("x =", x, ", y =", y);
print("x < y =", x < y);
print("x == y =", x == y);
print("x != y =", x != y);

print("Example 6 completed!");
